{
  "name": "photo-client",
  "version": "3.0.0",
  "description": "Ready-to-go Preact starter project powered by webpack.",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --debug --hot --output-pathinfo --watch --colors --inline --progress",
    "nodemon": "cross-env BABEL_ENV='node' nodemon src/server.js",
    "start": "superstatic build -p 8080 --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "dist": "cross-env NODE_ENV=production BABEL_ENV='node' babel src -d dist",
    "predist": "mkdir dist && ncp src/assets dist/assets && ncp src/config dist/config && ncp src/translation dist/translation",
    "prebuild": "mkdir build && ncp src/assets build/assets",
    "test": "npm run -s lint && npm run -s test:karma",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "lint": "eslint {src,test}"
  },
  "keywords": [
    "boilerplate",
    "webpack"
  ],
  "license": "MIT",
  "author": "Jason Miller <jason@developit.ca>",
  "devDependencies": {
    "autoprefixer": "^6.3.5",
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-intl": "^2.1.4",
    "babel-plugin-transform-decorators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-require-ignore": "0.0.2",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-minimal": "^2.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "chai": "^3.5.0",
    "core-js": "^2.2.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.14.0",
    "http-proxy-middleware": "^0.17.1",
    "json-loader": "^0.5.4",
    "karma": "^1.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.0.0",
    "ncp": "^2.0.0",
    "node-http-proxy": "^0.2.3",
    "nodemon": "^1.10.0",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.10.0",
    "raw-loader": "^0.5.1",
    "serve-favicon": "^2.3.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "superstatic": "^4.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "decko": "^1.1.3",
    "es6-promise": "^3.2.1",
    "escape-html": "^1.0.3",
    "isomorphic-fetch": "^2.2.1",
    "packery": "^2.1.1",
    "perfect-scrollbar": "^0.6.12",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-shallow-compare": "^15.3.1",
    "react-dom": "^15.3.0",
    "react-intl": "^2.1.4",
    "react-router": "^2.0.0"
  },
  "main": "src/index.js",
  "directories": {
    "test": "test"
  }
}
